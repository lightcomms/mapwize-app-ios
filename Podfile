    use_frameworks!
    pod 'Mapbox-iOS-SDK', '~> 3.7'
    pod 'MapwizeForMapbox', '1.0.5'
    pod 'GPSIndoorLocationProvider'
    pod 'ionicons'
    pod 'Kingfisher'
    pod 'MTBBarcodeScanner'
    pod 'CoreLightCom', '~> 1.0.4' 
end
